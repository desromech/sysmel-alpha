inline function decodeLinearChannelUnorm8(channel: UInt8) => Float32
    := channel / 255.0f.

inline function decodeSRGBChannelUnorm8(channel: UInt8) => Float32
    := srgbDecodeTable[channel].

inline function decodeSRGBUnorm8(color: UInt8x4) => Float32x4
    := Float32x4(decodeSRGBChannelUnorm8(color r), decodeSRGBChannelUnorm8(color g), decodeSRGBChannelUnorm8(color b), decodeLinearChannelUnorm8(color a)).

inline function encodeLinearChannelUnorm8(channel: Float32) => UInt8
    := channel *255.0f castTo: UInt8.

inline function encodeSRGBChannelUnorm8(channel: Float32) => UInt8
    := srgbEncodeTable[channel * 4095.0f castTo: UInt16].

inline function encodeSRGBUnorm8(color: Float32x4) => UInt8x4
    := UInt8x4(encodeSRGBChannelUnorm8(color r), encodeSRGBChannelUnorm8(color g), encodeSRGBChannelUnorm8(color b), encodeLinearChannelUnorm8(color a)).

