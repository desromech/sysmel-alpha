Compiler loadPackage: #Bootstrap.

if: Compiler hasStandardLibrary then: {
    Compiler loadPackage: #Stdn.

    if: Compiler hasRuntimeTypeInformation then: {
		if: Compiler hasGarbageCollectionSupport then: {
            Compiler loadPackage: #Smalltalk.
		}
    }.
}.

if: Compiler hasGPUSupport then: {
    Compiler loadPackage: #GPU.
}.
