namespace Smalltalk definition: {

## TODO: Use a global variable here.
gcclass Transcript definition: {
    meta definition: {
        method show: anObject
            := {Stdn stdout << anObject . self}.
        method cr
            := {Stdn stdout cr . self}.
        method nl
            := {Stdn stdout nl . self}.
    }
}.

}. ## End of namespace Smalltalk.
