## Evaluate the native stdlib
loadFileOnce: "native/native.sysmel".

## Evaluate the platform specific library bindings, that we depend on.
if: Compiler hasLibC then: {
    loadFileOnce: "libc/libc.sysmel".

    if: Compiler compilationTarget isUnix then: {
        loadFileOnce: "unix/unix.sysmel".
    }.

    if: Compiler compilationTarget isWin32 then: {
        loadFileOnce: "win32/win32.sysmel".
    }.
}.
