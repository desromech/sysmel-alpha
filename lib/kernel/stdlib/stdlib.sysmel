// Evaluate the platform specific library bindings, that we depend on.
Compiler
    evaluateFileNamed: "libc/libc.sysmel";
    yourself.

if: Compiler compilationTarget isUnix then: {
    Compiler evaluateFileNamed: "unix/unix.sysmel"
}.

if: Compiler compilationTarget isWin32 then: {
    Compiler evaluateFileNamed: "win32/win32.sysmel"
}.
