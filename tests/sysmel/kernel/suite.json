{
    "arguments": [
        "-nostdlib"
    ],

    "name": "Language Kernel",

    "cases": [
        {
            "name": "Primitive Int8",
            "eval": "Int8",
            "expected": "type Int8"
        },

        {
            "name": "Primitive UInt8",
            "eval": "UInt8",
            "expected": "type UInt8"
        },

        {
            "name": "Primitive Int16",
            "eval": "Int16",
            "expected": "type Int16"
        },

        {
            "name": "Primitive UInt16",
            "eval": "UInt16",
            "expected": "type UInt16"
        },

        {
            "name": "Primitive Int32",
            "eval": "Int32",
            "expected": "type Int32"
        },

        {
            "name": "Primitive UInt32",
            "eval": "UInt32",
            "expected": "type UInt32"
        },

        {
            "name": "Char is UInt8",
            "eval": "Char",
            "expected": "type UInt8"
        },

        {
            "name": "Char pointer",
            "eval": "Char pointer",
            "expected": "type UInt8 pointer"
        },

        {
            "name": "Integer Addition",
            "eval": "1+1",
            "expected": "2"
        },

        {
            "name": "Float addition",
            "eval": "1.0+1.0",
            "expected": "2.0"
        }
    ]
}
