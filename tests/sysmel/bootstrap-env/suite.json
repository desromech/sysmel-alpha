{
    "name": "Bootstrap Environment",
    "arguments": [
        "-nostdlib"
    ],

    "cases": [
        {
            "name": "Literal integer",
            "eval": "1",
            "expected": "1"
        },

        {
            "name": "Literal hex integer",
            "eval": "16r20",
            "expected": "32"
        },

        {
            "name": "Literal float",
            "eval": "1.0",
            "expected": "1.0"
        },

        {
            "name": "Literal Symbol",
            "eval": "#symbol",
            "expected": "#symbol"
        },

        {
            "name": "Literal String",
            "eval": "\"string\"",
            "expected": "\"string\""
        },

        {
            "name": "Literal Character",
            "eval": "'H'",
            "expected": "72"
        },

        {
            "name": "Named trait",
            "eval": "trait Type",
            "expected": "trait Type"
        },

        {
            "name": "Anon Trait",
            "eval": "trait",
            "expected": "trait __anon0"
        },

        {
            "name": "Anon Type",
            "eval": "type",
            "expected": "type __anon0"
        }

    ]
}
